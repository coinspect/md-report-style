flex-list()
    margin 0
    padding 0
    list-style none
    display flex
    flex-flow row nowrap
    flex 1 1 100%
    justify-content space-between
    li
        display flex


links()
    color $link-color
    &:hover
        color $hover-color


monospacedText()
    font $monospace-font
    font-size 0.8em

svgIcon()
    &::before
        mask-image url($svg)
        -webkit-mask-image url($svg)