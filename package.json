{
  "name": "md-report-style",
  "version": "1.2.1",
  "description": "CSS style for reports",
  "type": "module",
  "files": [
    "css/*.css",
    "styl/*.styl",
    "styl/lib/*"
  ],
  "scripts": {
    "build": "node fontConverter.js && stylus ./styl -c --include-css -o ./css",
    "watch": "node fontConverter.js && stylus ./styl -w -c --include-css -o ./css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coinspect/md-report-style.git"
  },
  "author": "emi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coinspect/md-report-style/issues"
  },
  "homepage": "https://github.com/coinspect/md-report-style#readme",
  "devDependencies": {
    "highlight.js": "^11.6.0",
    "stylus": "^0.58.1"
  },
  "dependencies": {
    "node-fetch": "^3.3.1"
  }
}
